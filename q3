
#include<stdio.h>
void  main()
{
  int i,n,a[100],S,R=0;
  printf("Enter the number of elements:\n") ;
  scanf("%d",&n) ;
  printf("Enter the elements\n") ;
    for(i=0;i<n;i++)
    {
     scanf("%d",&a[i]) ;
    }
  printf("Enter the element to be searched\n");
  scanf("%d",&S);
    for(i=0;i<n;i++)
    {
        if(a[i]==S)
        {
            printf("Element %d found at %d position\n",S,i);
            R=1;
            break;
        }
    }
    if(R==0)
    {
        printf("Element %d not found\n",S);
    }
}
